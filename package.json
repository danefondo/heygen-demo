{
  "name": "heygen-demo",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon --ignore 'heygen-client' app.js",
    "dev:client": "cd heygen-client && npm run serve",
    "build": "npm --prefix heygen-client install && npm --prefix heygen-client run build",
    "start": "node app.js"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5"
  }
}
